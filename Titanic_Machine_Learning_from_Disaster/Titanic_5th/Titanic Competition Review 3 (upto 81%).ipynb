    "        if ti in ['Don', 'Major', 'Capt', 'Jonkheer', 'Rev', 'Col', 'Sir']:\n",
    "            df.loc[df[\"Title\"] == ti, \"Title\"] = \"Mr\"\n",
    "        elif ti in ['the Countess', 'Mme', 'Lady', 'Dona']:\n",
    "            df.loc[df[\"Title\"] == ti, \"Title\"] = \"Mrs\"\n",
    "        elif ti in ['Mlle', 'Ms']:\n",
    "            df.loc[df[\"Title\"] == ti, \"Title\"] = \"Miss\"\n",
